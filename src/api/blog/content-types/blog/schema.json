{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "banner": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "author": {
      "type": "string"
    },
    "avatarAuthor": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "content": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "typeblogs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::typeblog.typeblog",
      "mappedBy": "blogs"
    }
  }
}
